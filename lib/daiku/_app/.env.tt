APP_NAME=<%= @app %>
APP_TITLE=<%= @app %>
ASSETS_BUILDER=<%= @assets %>
<% if @with_sidekiq -%>
AUTH_USERNAME=<%= @app %>
AUTH_PASSWORD=<%= @app %>
<% end -%>
<% if @datamapper -%>
DATABASE_URL=postgres://localhost/<%= @app %>
DB_NAME=<%= @app %>
<% end -%>
HONEYBADGER_PRIVATE_KEY=<%= @honeybadger_private_key %>
HONEYBADGER_PUBLIC_KEY=<%= @honeybadger_public_key %>
RACK_ENV=development
PORT=<%= @port %>
PUMA_MAX_THREADS=8
PUMA_MIN_THREADS=1
PUMA_WORKERS=0
